<%- include("partials/header") %>

  <h1>Home</h1>
  <p><%= hometext %></p>

  <% posts.forEach(function(post){ %>
    <h2><%= post.title %></h2>
    <p>
      <%= post.body.substring(1, 100) + "..." %>
      <a href="/posts/<%= post.title %>">read more</a>
    </p>
  <% }) %>


<%- include("partials/footer") %>
